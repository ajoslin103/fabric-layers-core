{
  "name": "react-vue-electron",
  "version": "0.1.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "ajoslin103",
  "homepage": "https://ajoslin103.github.io",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --win --dir",
    "build:all": "rm -rf dist && npm run build:win && npm run build:mac && npm run build:mmac",
    "build:win": "npm run build && electron-builder --win --x64 && post-build-win.sh",
    "build:mac": "npm run build && electron-builder --mac --x64 && post-build-mac.sh",
    "build:mmac": "npm run build && electron-builder --mac --arm64 && post-build-mmac.sh",
    "start": "npm run typecheck && electron-vite preview",
    "build": "npm run typecheck && electron-vite build",
    "dev": "npm run typecheck && electron-vite dev"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@pdftron/webviewer": "^11.1.1",
    "autoprefixer": "^10.0.0",
    "electron-conf": "^1.2.1",
    "electron-updater": "^6.1.7",
    "electron-winston": "^1.0.0",
    "postcss": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@aurelia/vite-plugin": "^2.0.0-beta.22",
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/lodash": "^4.17.14",
    "@types/node": "^20.14.8",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.9",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vue": "^3.4.30",
    "vue-tsc": "^2.0.22"
  }
}
