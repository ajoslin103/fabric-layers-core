{
  "name": "fabric-layers-react",
  "version": "1.0.1",
  "description": "A React wrapper around the fabric-layers-core library",
  "main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build:webpack": "webpack --config webpack.config.js",
    "build": "npm run build:types && npm run build:webpack",
    "prepare": "npm run build",
    "dev": "yarn serve",
    "serve": "webpack serve --config webpack.demo.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajoslin103/fabric-layers-react.git"
  },
  "keywords": [
    "fabric",
    "fabricjs",
    "layers",
    "react",
    "canvas",
    "drawing"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "fabric": "^6.7.0",
    "fabric-layers-core": "^1.1.3",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "fabric": {
      "optional": true
    },
    "fabric-layers-core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/fabric": "^5.3.10",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "fabric-layers": "^1.1.3",
    "html-webpack-plugin": "^5.6.3",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "fabric-layers-core": "^1.1.6"
  }
}
